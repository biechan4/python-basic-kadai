        "# 台形の各辺と高さ(cm)\n",
        "top = 10    # 上辺\n",
        "bottom = 20 # 下辺\n",
        "hight = 5   #高さ\n",
        "area = (top + bottom) * hight / 2\n",
        "\n",
        "# 面積の公式と結果\n",
        "print(f\"公式：台形の面積 =（上辺({top}) ＋ 下辺({bottom})）× 高さ({hight}) ÷ 2\")\n",
        "print(\"面積 = \" + str(area) + \"cm²\")\n"
      ]
    }
